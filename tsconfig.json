{
  "compilerOptions": {
    "target": "es5",
    "rootDir": "./lib",
    "outDir": "./dist",
    "baseUrl": "./lib",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true
  },
  "include": [
    "lib/**/*.ts"
  ]
}
